#include<bits/stdc++.h>
using namespace std;

int main()
{
    int n,t;
    cin>>n;
    vector<int> v;
    int k = 0;
    for(int i=0;i<n;i++)
    {
        cin>>t;
        if(t==1)
        {
            k++;
        }
        else
        {
            k=0;
        }
        v.push_back(t);
        if(k==5)
        {
            v.push_back(0);
            k=0;
        }
    }
    for( auto t: v)
    {
        cout<<t<<" ";
    }
    return 0;
}
